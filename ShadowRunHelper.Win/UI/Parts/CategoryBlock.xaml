                    <TextBlock Grid.Column="1">
                        <Run x:Uid="Model_Thing_Wert_W"/>
                        <Run x:Uid="Space"/>
                        <Run x:Uid="KlammerAufRund"/>
                        <Run x:Uid="Model__Pool"/>
