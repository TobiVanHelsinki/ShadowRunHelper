                    <TextBlock Grid.Column="3" x:Uid="Model_Zaubersprueche_Entzug"  Style="{StaticResource Category_Head_Items}"/>
                    <TextBlock Grid.Column="4" x:Uid="Model_Thing_Wert"  Style="{StaticResource Category_Head_Items}"/>
                </Grid>
