                        <ColumnDefinition Width="58"/>
                        <ColumnDefinition Width="4*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="58"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Grid.Column="0" x:Uid="Model_Item_Aktiv"  Style="{StaticResource Category_Head_Items}"/>
                    <TextBlock Grid.Column="1" x:Uid="Model_Item_Besitz"  Style="{StaticResource Category_Head_Items}"/>
                    <TextBlock Grid.Column="2" x:Uid="Model_Thing_Bezeichner"  Style="{StaticResource Category_Head_Items}"/>
                    <TextBlock Grid.Column="3" x:Uid="Model_Thing_Wert"  Style="{StaticResource Category_Head_Items}"/>
                    <TextBlock Grid.Column="4" x:Uid="Model_Geist_Dienste" Style="{StaticResource Category_Head_Items}"/>
