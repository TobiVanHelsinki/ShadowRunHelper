                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Style="{StaticResource Category_Head_Items}">
                        <Run x:Uid="Model_Thing_Bezeichner"/>
                        <Run x:Uid="Bindestrich"/>
                        <Run x:Uid="Model_Thing_Wert"/>
                    </TextBlock>
                    <TextBlock Grid.Column="1" x:Uid="Model_Thing_Notiz" Style="{StaticResource Category_Head_Items}" HorizontalAlignment="Right"/>
                </Grid>
            </DataTemplate>
            <DataTemplate x:Name="Eigenschaft_E" x:Key="Eigenschaft_E">
                <Grid Style="{StaticResource Category_Head_Items_Grid}">
